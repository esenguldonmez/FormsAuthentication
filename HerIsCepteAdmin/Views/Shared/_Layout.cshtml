@using HerIsCepteAdmin.DAL.Security
@{ var userName = HttpContext.Current.User.Identity.Name;
    var userEmail = (HttpContext.Current.User.Identity as ICustomIdentity)?.Email.ToString(); }
<!DOCTYPE html>
<html dir="ltr" lang="en">
<head>
